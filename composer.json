{
  "require": {
    "slim/slim": "^4.10",
    "slim/psr7": "^1.5",
    "php-di/php-di": "^6.3",
    "jenssegers/blade": "^1.4",
    "php-di/slim-bridge": "^3.0",
    "ext-json": "*",
    "zeuxisoo/slim-whoops": "^0.7.3",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "files": [
      "./app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app",
      "Boot\\": "bootstrap"
    },
    "scripts": {
      "post-root-package-install": [
        "php -r \"file_exists(.env) || copy('.env.example', '.env');\""
      ]
    }
  }
}
